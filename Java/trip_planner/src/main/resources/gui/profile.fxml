<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.layout.*?>

<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="logic.profileController"
      prefHeight="400.0" prefWidth="600.0">

    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label text="Your trip: " styleClass="title"/>
    <Label fx:id="priceLabel" />

    <TableView fx:id="table">
        <columns>
            <TableColumn text="Name" fx:id="name"/>
            <TableColumn text="Price" fx:id="price"/>
        </columns>
    </TableView>

    <Button text="Back to search" onAction="#onClickReturnToProject"/>
</VBox>
